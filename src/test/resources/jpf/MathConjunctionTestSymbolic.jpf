# JPF configuration for Math.requireBothNonNegative(a, b) - single if with &&
# Run with: ./tools/run-jpf.sh MathConjunctionTestSymbolic.jpf

@include = ${config_path}/../../../../tools/jpf-bundled.properties

target = br.ufpe.cin.witup.jpf.MathConjunctionJPFDriverSymbolic
listener = br.ufpe.cin.witup.jpf.ExceptionConditionListener

# Symbolic parameters for run(a, b)
symbolic.method = br.ufpe.cin.witup.jpf.MathConjunctionJPFDriverSymbolic.run(sym#sym)

search.multiple_errors = true

symbolic.min_int = -100
symbolic.max_int = 100
