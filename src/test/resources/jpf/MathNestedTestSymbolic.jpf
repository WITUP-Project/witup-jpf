# JPF configuration for Math.requireBothNonNegativeNested(a, b) - two nested ifs
# Run with: ./tools/run-jpf.sh MathNestedTestSymbolic.jpf

@include = ${config_path}/../../../../tools/jpf-bundled.properties

target = br.ufpe.cin.witup.jpf.MathNestedJPFDriverSymbolic
listener = br.ufpe.cin.witup.jpf.ExceptionConditionListener

# Symbolic parameters for run(a, b)
symbolic.method = br.ufpe.cin.witup.jpf.MathNestedJPFDriverSymbolic.run(sym#sym)

search.multiple_errors = true

symbolic.min_int = -100
symbolic.max_int = 100
